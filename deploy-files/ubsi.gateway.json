{
  "maven_url": "${local-maven-repo}",
  "maven_repo": [ {
    "url" : "https://maven.pkg.github.com/ubsi-home/release/",
    "user": "${github-account}",
    "password": "${github-pat}"
  } ],
  "service_name": "rewin.ubsi.gateway",
  "service_class": "rewin.service.ubsi.gateway.Service",
  "jar_group": "rewin.service.ubsi",
