    "artifactId": "rewin.service.ubsi.gateway",
    "version": "1.0.0"
  },
  {
    "jarFile": "rewin.ubsi.core-2.3.0.jar",
    "groupId": "rewin.ubsi",
    "artifactId": "rewin.ubsi.core",
    "version": "2.3.0"
  }
]
}

